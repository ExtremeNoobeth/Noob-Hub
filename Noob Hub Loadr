local function ACLV_fake_script() -- load.loadhub 
	local script = Instance.new('LocalScript', load)

	local frame = script.Parent.Parent
	local gui = script.Parent.Parent.Parent
	
	script.Parent.MouseButton1Click:Connect(function()
		frame:TweenPosition(
			UDim2.new(0.5,0,1.25,0),
			"In",
			"Quart",
			0.75,
			true
		)
		wait(.75)
		if _G.nhv1 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/24UbBNp3"))();
		elseif _G.nhv2 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/Bw5aM9Ch"))();
		elseif _G.nhv3 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/UdqLDk8Y"))();
		elseif _G.nhv4 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/yJmeNL3R"))();
		elseif _G.nhv5 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/QA3LqHyZ"))();
		elseif _G.nhv6 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/bsKdpLiY"))();
		elseif _G.nhv7 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/NMC6dqHw"))();
		elseif _G.nhv8 == 1 then
			loadstring(game:HttpGet("https://pastebin.com/raw/nxfbPLnD"))();
		end
		gui:Destroy()
	end)
end
coroutine.wrap(ACLV_fake_script)()
local function XUYRNQ_fake_script() -- selected.update 
	local script = Instance.new('LocalScript', selected)

	local text = script.Parent
	
	while wait() do
		if _G.nhv1 == 1 then
			text.Text = "Selected - V1"
		elseif _G.nhv2 == 1 then
			text.Text = "Selected - V2"
		elseif _G.nhv3 == 1 then
			text.Text = "Selected - V3"
		elseif _G.nhv4 == 1 then
			text.Text = "Selected - V4"
		elseif _G.nhv5 == 1 then
			text.Text = "Selected - V5"
		elseif _G.nhv6 == 1 then
			text.Text = "Selected - V6"
		elseif _G.nhv7 == 1 then
			text.Text = "Selected - V7"
		elseif _G.nhv8 == 1 then
			text.Text = "Selected - V8"
		end
	end
end
coroutine.wrap(XUYRNQ_fake_script)()
local function KDOK_fake_script() -- main.setup 
	local script = Instance.new('LocalScript', main)

	local frame = script.Parent
	frame.Position = UDim2.new(0.5,0,1.25,0)
	
	frame:TweenPosition(
		UDim2.new(0.5,0,0.5,0),
		"Out",
		"Quart",
		0.75,
		true
	)
end
coroutine.wrap(KDOK_fake_script)()
