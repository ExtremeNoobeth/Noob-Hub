-- Scripts:

local function DEAVMJU_fake_script() -- v1.activate 
	local script = Instance.new('LocalScript', v1)

	_G.nhv1 = 0
	
	script.Parent.MouseButton1Click:Connect(function()
		_G.nhv1 = 1
		_G.nhv2 = 0
		_G.nhv3 = 0
		_G.nhv4 = 0
		_G.nhv5 = 0
		_G.nhv6 = 0
		_G.nhv7 = 0
		_G.nhv8 = 0
	end)
end
coroutine.wrap(DEAVMJU_fake_script)()
local function BKDC_fake_script() -- v8.activate 
	local script = Instance.new('LocalScript', v8)

	_G.nhv8 = 1
	
	script.Parent.MouseButton1Click:Connect(function()
		_G.nhv1 = 0
		_G.nhv2 = 0
		_G.nhv3 = 0
		_G.nhv4 = 0
		_G.nhv5 = 0
		_G.nhv6 = 0
		_G.nhv7 = 0
		_G.nhv8 = 1
	end)
end
coroutine.wrap(BKDC_fake_script)()
